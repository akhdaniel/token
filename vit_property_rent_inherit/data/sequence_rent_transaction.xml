<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data >
		<function name="write" model="ir.model.data">
			<function name="search" model="ir.model.data">
				<value eval="[('module', '=', 'vit_property_rent'), ('name', '=', 'sequence_rent_transaction')]"/>
			</function>
			<value eval="{'noupdate': False}" /> 
		</function>
		<record id="vit_property_rent.sequence_rent_transaction" model="ir.sequence">
			<field name="name">sequence_rent_transaction</field>
			<field name="code">vit.rent_transaction</field>
			<field name="prefix">RENXX/%(year)s/%(month)s/</field>
			<field name="padding">3</field>
		</record>
		<function name="write" model="ir.model.data">
			<function name="search" model="ir.model.data">
				<value eval="[('module', '=', 'vit_property_rent'), ('name', '=', 'sequence_rent_transaction')]"/>
			</function>
			<value eval="{'noupdate': True}" />
		</function>
	</data>
</odoo>