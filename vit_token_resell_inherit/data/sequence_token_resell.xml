<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data >
		<function name="write" model="ir.model.data">
			<function name="search" model="ir.model.data">
				<value eval="[('module', '=', 'vit_token_resell'), ('name', '=', 'sequence_token_resell')]"/>
			</function>
			<value eval="{'noupdate': False}" /> 
		</function>
		<record id="vit_token_resell.sequence_token_resell" model="ir.sequence">
			<field name="name">sequence_token_resell</field>
			<field name="code">vit.token_resell</field>
			<field name="prefix">TOKXX/%(year)s/%(month)s/</field>
			<field name="padding">3</field>
		</record>
		<function name="write" model="ir.model.data">
			<function name="search" model="ir.model.data">
				<value eval="[('module', '=', 'vit_token_resell'), ('name', '=', 'sequence_token_resell')]"/>
			</function>
			<value eval="{'noupdate': True}" />
		</function>
	</data>
</odoo>